{
  "name": "react-component-setup",
  "version": "0.0.3",
  "description": "A react component test helper",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "scripts": {
    "build": "rollup -c",
    "start": "rollup -c -w",
    "test": "CI=1 react-scripts test --env=jsdom",
    "test:cov": "npm test -- --coverage",
    "coverage": "npm run test:cov && cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kpfromer/react-component-setup.git"
  },
  "keywords": [
    "jest",
    "react",
    "reactjs",
    "test",
    "testing",
    "component",
    "mocha",
    "chai"
  ],
  "author": "Kyle Pfromer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kpfromer/react-component-setup/issues"
  },
  "homepage": "https://github.com/kpfromer/react-component-setup#readme",
  "peerDependencies": {
    "react": "^0.14.9 || ^15.0.0 || ^16.0.0",
    "react-dom": "^0.14.9 || ^15.0.0 || ^16.0.0",
    "enzyme": "~3.3.0"
  },
  "devDependencies": {
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-scripts": "^1.1.4",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "coveralls": "^3.0.1",
    "babel-cli": "^6.26.0",
    "babel-jest": "^22.4.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "rollup": "^0.60.7",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-peer-deps-external": "^2.1.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/setupTests.js",
      "!<rootDir>/node_modules/",
      "!<rootDir>/path/to/dir/"
    ]
  }
}
